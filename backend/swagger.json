{
    "openapi": "3.0.0",
    "info": {
        "title": "API Front-end Project",
        "version": "1.0.0",
        "description": "A"
    },
    "servers": [
        {
            "url": "http://localhost:3000",
            "description": "Servidor local"
        }
    ],
    "paths": {
        "/users": {
            "get": {
                "summary": "Retorna os usuários",
                "responses": {
                    "200": {
                        "description": "Resposta bem sucedida",
                        "content": {
                            "application/json": [
                                {
                                    "name": "Bruno",
                                    "age": 17
                                }
                            ]
                        }
                    }
                }
            },
            "post": {
                "summary": "Adiciona os usuários",
                "responses": {
                    "201": {
                        "description": "Adiciona os usuários"
                    }
                }
            }
        }
    }
}